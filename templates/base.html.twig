<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
        <meta name="description" content="">
        <meta name="author" content="Romdhan KHOUAJA">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {# Run `composer require symfony/webpack-encore-bundle`
           and uncomment the following Encore helpers to start using Symfony UX #}
        {% block stylesheets %}
            {#{{ encore_entry_link_tags('app') }}#}
            <link rel="stylesheet" href="/bootstrap/4.6.0/css/bootstrap.css"> 
            <meta name="theme-color" content="#563d7c">
            <style>
                .bd-placeholder-img {
                    font-size: 1.125rem;
                    text-anchor: middle;
                    -webkit-user-select: none;
                    -moz-user-select: none;
                    -ms-user-select: none;
                    user-select: none;
                }

                @media (min-width: 768px) {
                    .bd-placeholder-img-lg {
                    font-size: 3.5rem;
                    }
                }
            </style>
        {% endblock %}

        {% block javascripts %}
            {#{{ encore_entry_script_tags('app') }}#}
            <script src="/jquery/3.5.1/jquery-3.5.1.js"></script>
            <script src="/bootstrap/4.6.0/js/bootstrap.bundle.js"></script>

            {# <script src="{{ asset('fontawesome-free-5.11.2-web/js/all.min.js') }}"></script>
            <script src="{{ asset('select2/dist/js/select2.full.min.js') }}"></script>
            <script src="{{ asset('select2/dist/js/i18n/fr.js') }}"></script>
            <script>
                $('select').select2({
                    theme: 'bootstrap4',
                    language: "fr",
                });  
            </script>         #}
        {% endblock %}
    </head>
    <body>
        <div class="container">
            {% block body %}{% endblock %}
        </div>        
    </body>
</html>
